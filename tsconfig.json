{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": false,
      "noImplicitAny": false,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      },
      "lib": ["es5", "es2015", "dom"]
  },
  "include": [
      "./src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}
